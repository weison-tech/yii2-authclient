{
    "name": "weison-tech/yii2-authclient",
    "description": "External authentication via OAuth and OpenID for the Yii framework",
    "keywords": ["yii2", "OAuth", "OpenID", "auth"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/weison-tech/yii2-authclient/issues?state=open",
        "source": "https://github.com/weison-tech/yii2-authclient"
    },
    "authors": [
        {
            "name": "xiaomalover",
            "email": "xiaomalover@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": { "yii\\authclient\\": "" }
    }
}
